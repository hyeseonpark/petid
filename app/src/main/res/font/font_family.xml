<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--light-->
    <font
        android:font="@font/noto_400"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/noto_400"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!--regular-->
    <font
        android:font="@font/noto_500"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/noto_500"
        app:fontStyle="normal"
        app:fontWeight="500" />

    <font
        android:font="@font/noto_600"
        android:fontStyle="normal"
        android:fontWeight="600"
        app:font="@font/noto_600"
        app:fontStyle="normal"
        app:fontWeight="600" />

    <font
        android:font="@font/noto_700"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/noto_700"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>